{
  "name": "tintnaingwin/kuu-pyaung",
  "description": "Laravel package to convert files from zawgyi to unicode",
  "keywords": [
    "myanmar-font",
    "unicode",
    "zawgyi",
    "convert",
    "resource",
    "database",
    "kuu-pyaung"
  ],
  "homepage": "https://github.com/tintnaingwin/kuu-pyaung",
  "license": "MIT",
  "authors": [
    {
      "name": "Tint Naing Win",
      "email": "amigo.k8@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "illuminate/console": "~5.2|~5.3|~5.4",
    "illuminate/contracts": "~5.2|~5.3|~5.4",
    "illuminate/events": "~5.2|~5.3|~5.4",
    "illuminate/filesystem": "~5.2|~5.3|~5.4",
    "illuminate/support": "~5.2|~5.3|~5.4",
    "tintnaingwin/myanfont": "^0.2",
    "doctrine/dbal": "^2.5"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "~3.2|~3.3|~3.4",
    "phpunit/phpunit": "^4.8|^5.7"
  },
  "autoload": {
    "psr-4": {
      "Tintnaingwin\\KuuPyaung\\": "src"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tintnaingwin\\KuuPyaung\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tintnaingwin\\KuuPyaung\\KuuPyaungServiceProvider"
      ]
    }
  }
}
