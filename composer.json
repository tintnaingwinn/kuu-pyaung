{
  "name": "tintnaingwin/kuu-pyaung",
  "description": "Laravel package to convert files from zawgyi to unicode",
  "keywords": [
    "myanmar-font",
    "unicode",
    "zawgyi",
    "convert",
    "resource",
    "database",
    "kuu-pyaung"
  ],
  "homepage": "https://github.com/tintnaingwin/kuu-pyaung",
  "license": "MIT",
  "authors": [
    {
      "name": "Tint Naing Win",
      "email": "amigo.k8@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/console": "~5.8.0|^6.0",
    "illuminate/contracts": "~5.8.0|^6.0",
    "illuminate/events": "~5.8.0|^6.0",
    "illuminate/filesystem": "~5.8.0|^6.0",
    "illuminate/support": "~5.8.0|^6.0",
    "tintnaingwin/myanfont": "2.*"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "~3.8.0|^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Tintnaingwin\\KuuPyaung\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tintnaingwin\\KuuPyaung\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tintnaingwin\\KuuPyaung\\KuuPyaungServiceProvider"
      ]
    }
  }
}
